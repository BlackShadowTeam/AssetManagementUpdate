@model IEnumerable<Asset.Models.Library.EntityModels.OrganizationModels.Branch>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Branchs</h2>

<p>@Html.ActionLink("New Branch", "New", "Branchs", null, new {@class="btn btn-primary"})</p>

<table id="branchTable" class="table table-bordered table-hover">
    <thead>
    <tr>
        <th>Name</th>
        <th>Short Name</th>
        <th>Branch Code</th>
        <th>Organization Name</th>
        <th>Action</th>
    </tr>
    </thead>
    
    <tbody></tbody>

</table>

@section scripts
{
    <script src="~/Scripts/FormScript/branch.js"></script>
    
}