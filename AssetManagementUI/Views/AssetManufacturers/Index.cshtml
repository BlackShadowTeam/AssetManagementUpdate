
@{
    ViewBag.Title = "Asset Manufacturer";
    ViewBag.ManuHead = "Asset Setup";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Asset Manufacturers</h2>

<p>@Html.ActionLink("New Manufacturer", "New", "AssetManufacturers", null, new { @class = "btn btn-primary" })</p>

<table id="assetManufacturerTable" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Name</th>
            <th>Short Name</th>
            <th>Code</th>
            <th>Asset Group</th>
            <th>Description</th>
            <th>Action</th>
        </tr>
    </thead>

    <tbody></tbody>

</table>

@section scripts
{
    <script src="~/Scripts/FormScript/assetManufacturer.js"></script>

}