@{
    ViewBag.Title = "Asset Model List";
}
<p>@Html.ActionLink("New Model", "New", "AssetModels", null, new { @class = "btn btn-primary" })</p>
<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 ">
        <div class="box box-default">
            <div class="box-header">
                <h3 class="box-title">Model List</h3>
            </div>
            <div class="box-body">
                <table id="assetModelTable" class="table table-bordered table-hover">
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th>Short Name</th>
                            <th>Code</th>
                            <th>Group</th>
                            <th>Manufacturer</th>
                            <th>Description</th>
                            <th>Action</th>
                        </tr>
                    </thead>

                    <tbody></tbody>

                </table>
            </div>
        </div>
    </div>
</div>

@section scripts
{
    <script src="~/Scripts/FormScript/assetModel.js"></script>

}

