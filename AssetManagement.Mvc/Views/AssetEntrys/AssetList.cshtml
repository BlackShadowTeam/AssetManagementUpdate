@{
    ViewBag.Title = "Asset List";
}

<p>@Html.ActionLink("New Asset Entry", "New", "AssetEntrys", null, new { @class = "btn btn-primary" })</p>
<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 ">
        <div class="box box-default">
            <div class="box-header">
                <h3 class="box-title">Asset List</h3>
            </div>
            <div class="box-body">
                <table id="assetEntryTable" class="table table-bordered table-hover">
                    <thead>
                        <tr>
                            <th>Asset Id</th>
                            <th>Name</th>
                            <th>Serial No</th>
                            <th>Status</th>
                            <th>Action</th>
                        </tr>
                    </thead>

                    <tbody></tbody>

                </table>
            </div>
        </div>
    </div>
</div>

@section scripts
{
    <script src="~/Scripts/FormScript/assetEntry.js"></script>

}
