@{
    ViewBag.Title = "Employee List";
}
<p>@Html.ActionLink("New Employee", "New", "Employees", null, new { @class = "btn btn-primary" })</p>
<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 ">
        <div class="box box-default">
            <div class="box-header">
                <h3 class="box-title">Employee List</h3>
            </div>
            <div class="box-body">
                <table id="employeeTable" class="table table-bordered table-hover">
                    <thead>
                        <tr>
                            <th>Code</th>
                            <th>Organization</th>
                            <th>Branch</th>
                            <th>Department</th>
                            <th>Designation</th>
                            <th>First Name</th>
                            <th>Last Name</th>
                            <th>Contact No</th>
                            <th>Email</th>
                            <th>Action</th>
                        </tr>
                    </thead>

                    <tbody></tbody>

                </table>
            </div>
        </div>
    </div>
</div>

@section scripts
{
    <script src="~/Scripts/FormScript/employee.js"></script>

}